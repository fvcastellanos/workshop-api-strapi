{
  "kind": "collectionType",
  "collectionName": "operation_types",
  "info": {
    "name": "Operation Type"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 150
    },
    "description": {
      "type": "string",
      "maxLength": 300
    },
    "inbound": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "active": {
      "type": "boolean",
      "default": true,
      "required": true
    }
  }
}
